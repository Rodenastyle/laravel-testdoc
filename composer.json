{
  "name": "rodenastyle/laravel-testdoc",
  "description": "Laravel Functional Tests Docs Generator",
  "keywords": ["laravel", "docs", "generator", "functional", "tests", "tdd", "bdd", "swagger", "openapi3"],
  "license": "MIT",

  "authors": [
    {
      "name": "Sergio RÃ³denas",
      "homepage": "https://rodenastyle.com",
      "role": "developer"
    }
  ],

  "require": {
    "php": "^7.1",
    "ext-json": "*",
    "illuminate/contracts": "^5.7",
    "illuminate/support": "^5.7",
    "zircote/swagger-php": "^3.0"
  },

  "autoload": {
    "psr-4": {
      "Rodenastyle\\TestDoc\\": "src"
    }
  },

  "autoload-dev": {
    "psr-4": {
      "Rodenastyle\\TestDoc\\Test\\": "tests"
    }
  },

  "extra": {
    "laravel": {
      "providers": [
        "Rodenastyle\\TestDoc\\ServiceProvider"
      ]
    }
  },

  "scripts": {
    "test": "vendor/bin/phpunit"
  },

  "config": {
    "sort-packages": true
  }
}
